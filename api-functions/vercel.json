      "destination": "/api/admin-upload-tokens"
    },
    {
      "source": "/api/admin/manage-users",
      "destination": "/api/admin-manage-users"
    },
    {
      "source": "/api/admin/blocked-users",
      "destination": "/api/admin-blocked-users"
    },
    {
