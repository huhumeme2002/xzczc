{
  "version": 2,
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, X-Requested-With, Accept, Origin, User-Agent, DNT, Cache-Control, X-Mx-ReqToken, Keep-Alive, X-Requested-With, If-Modified-Since, X-CSRF-Token"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Max-Age",
          "value": "86400"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/auth/register",
      "destination": "/api/register-db"
    },
    {
      "source": "/api/auth/login", 
      "destination": "/api/login-db"
    },
    {
      "source": "/api/keys/redeem", 
      "destination": "/api/redeem-key"
    },
    {
      "source": "/api/test-database", 
      "destination": "/api/test-db"
    },
    {
      "source": "/api/simple-test", 
      "destination": "/api/simple-test"
    },
    {
      "source": "/api/test-connect", 
      "destination": "/api/test-connect"
    },
    {
      "source": "/api/init-database", 
      "destination": "/api/init-db"
    },
    {
      "source": "/api/admin/create-admin",
      "destination": "/api/create-admin"
    },
    {
      "source": "/api/admin/users",
      "destination": "/api/admin-users"
    },
    {
      "source": "/api/admin/analytics",
      "destination": "/api/admin-analytics"
    },
    {
      "source": "/api/admin/keys",
      "destination": "/api/admin-keys"
    },
    {
