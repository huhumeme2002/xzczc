      "destination": "/api/redeem-token"
    },
    {
      "source": "/api/create-test-token",
      "destination": "/api/create-test-token"
    },
    {
      "source": "/api/fix-database-schema",
      "destination": "/api/fix-database-schema"
    },
    {
      "source": "/api/generate-token",
      "destination": "/api/generate-token"
    },
    {
      "source": "/api/generate-login-code",
      "destination": "/api/generate-login-code"
    },
    {
      "source": "/api/get-daily-login",
